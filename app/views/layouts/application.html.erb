<!DOCTYPE html>
<html>
<head>
  <title>PandSoft </title>
  <%= favicon_link_tag '/assets/favicon.ico' %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="http://mymaplist.com/js/vendor/TweenLite.min.js"></script>
  <%= csrf_meta_tags %>
</head>
<body>
   <div class="navbar navbar-default navbar-fixed-top" role="navigation">
     <div class="container">
       <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
           </button>
          <%= link_to "PandSoft" , users_dashboard_path , method: :get, :class => "navbar-brand" %>
        </div>
            <div class="navbar-collapse collapse">
              <ul class="nav navbar-nav">
                
                <% if signed_in? %>
                <li><%= link_to "Stories" , users_home_path , method: :get  %></li>
                <li><%= link_to "My Life" , folders_path %></li>
                <li>
                <div class="col-sm-5 col-md-5">
                  <%= form_tag users_home_path, :method => 'get' ,:class =>"navbar-form"  do %>
                  <p> 
                    <div class="input-group">
                        <%= text_field_tag :search, params[:search], placeholder: "Search stories",:class=>"form-control" , :size => "70"%>
                    <div class="input-group-btn">
                          <%= submit_tag "Search", :name => nil ,:class => "btn btn-success" ,:size=> "10" %>
                    </div>
                    </div>
                  </p>
                  <% end %>
                </div>
                </li>
        <% end %>
             
        </ul>
      <ul class="nav navbar-nav navbar-right">
        <% if signed_in? %>
          <li><%= link_to "SignOut" ,destroy_user_session_path , :method=>:delete %>
          </li>
        
        <% else %>
            
          <li><%= link_to "Register", new_user_registration_path  %></li>
          <li><%= link_to "Sign In", new_user_session_path%></li>
        
        <% end %>
                  
      </ul>
    </div>
  </div>
      

      </div>

<% flash.each do |type, message| %>
        <div class=" alert alert-<%= bootstrap_class_for(type) %>">
        <button class="close" data-dismiss="alert">Ã—</button>
        <%= message %>
        </div>
<% end %>

<%= yield %>

<hr>

